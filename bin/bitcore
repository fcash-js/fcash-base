#!/usr/bin/env node

if (parseInt(process.version.split('.')[0].slice(1)) < 8) {
  throw new Error('Fcash requires Node version 8 or higher.');
}

var fcash_base = require('fcash-node/lib/cli/fcash-base');
var path = require('path');
var servicesPath = path.resolve(__dirname, '../');
var additionalServices = ['insight-api', 'insight-ui'];
fcash_base(servicesPath, additionalServices);
